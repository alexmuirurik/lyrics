{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"typeRoots": ["./node_modules/@types", "./types"],
		"strict": true,
		"paths": {
			"@/*": [
				"./*"
			],
			"tailwind.config": [
				"./tailwind.config.js"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"nativewind-env.d.ts"
	]
}